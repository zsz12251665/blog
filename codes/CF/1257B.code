var
	t,ti:integer;
	x,y:longint;
begin// No comments available
	read(t);
	for ti:=1 to t do
		begin
			read(x,y);
			if (x>=y) or (x>3) or (x=2) and (y=3) then
				writeln('YES')
			else
				writeln('NO');
		end;
end.

#include<cstdio>
using namespace std;
int main()// No comments available
{
	int t;
	for(scanf("%d",&t);t>0;t--)
	{
		long x,y;
		scanf("%ld%ld",&x,&y);
		printf((x>=y || x>3 || x==2 && y==3)?"YES\n":"NO\n");
	}
	return 0;
}
